    "gatsby-transformer-remark": "^5.15.1",
    "gatsby-transformer-sharp": "^4.15.1",
    "gh-pages": "^4.0.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.28.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0", 
    "react-dom": "^18.2.0", 
    "react-helmet": "^6.1.0",
    "react-transition-group": "^4.4.5",
    "scrollreveal": "^4.0.9",
