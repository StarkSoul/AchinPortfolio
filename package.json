    "gatsby-plugin-google-analytics": "^4.15.1",
    "gatsby-plugin-manifest": "^4.15.1",
    "gatsby-plugin-netlify": "^2.1.27",
    "gatsby-plugin-offline": "^5.15.1",
    "gatsby-plugin-react-helmet": "^5.15.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^4.15.1",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-styled-components": "^5.15.1",
    "gatsby-remark-external-links": "^6.15.1",
    "gatsby-remark-images": "^6.15.1",
    "gatsby-remark-prismjs": "^6.15.1",
